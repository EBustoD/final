<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="org.tensorflow.codelabs.objectdetection.scanner_numSerie">

    <ImageView
        android:id="@+id/imageViewRecorteSerie"
        android:layout_width="match_parent"
        android:layout_height="264dp"
        android:layout_marginTop="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />
    <TextView
        android:id="@+id/txtLecturaNumeroSerie"
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:layout_marginTop="290dp"
        android:background="@drawable/boarder"
        android:hint="Numero serie" />

    <ImageView
        android:id="@+id/imageViewRecorteConsumo"
        android:layout_width="match_parent"
        android:layout_height="264dp"
        android:layout_marginTop="360dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/txtLecturaConsumo"
        android:layout_width="match_parent"
        android:layout_height="57dp"
        android:layout_marginTop="635dp"
        android:background="@drawable/boarder"
        android:hint="Consumo" />



</RelativeLayout>