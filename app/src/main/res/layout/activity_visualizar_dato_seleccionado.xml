<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="org.tensorflow.codelabs.objectdetection.scanner_numSerie">

    <Button
        android:id="@+id/btnBack"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="10dp"
        android:onClick="onClick"
        app:backgroundTint="@color/amvisa"
        app:icon="@drawable/abc_vector_test"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/imageViewRecorteSerie"
        android:layout_width="match_parent"
        android:layout_height="234dp"
        android:layout_marginTop="45dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/txtLecturaNumeroSerie"
        android:layout_width="290dp"
        android:layout_height="57dp"
        android:layout_marginTop="290dp"
        android:layout_marginLeft="10dp"
        android:gravity="center"
        android:background="@drawable/boarder"
        android:hint="Numero serie" />

    <TextView
        android:id="@+id/txteditadoSerie"
        android:layout_width="90dp"
        android:layout_height="57dp"
        android:layout_marginStart="310dp"
        android:layout_marginTop="290dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/boarder"
        android:gravity="center" />

    <ImageView
        android:id="@+id/imageViewRecorteConsumo"
        android:layout_width="match_parent"
        android:layout_height="244dp"
        android:layout_marginTop="380dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

    <TextView
        android:id="@+id/txtLecturaConsumo"
        android:layout_width="290dp"
        android:layout_height="57dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="635dp"
        android:background="@drawable/boarder"
        android:gravity="center"
        android:hint="Consumo" />

    <TextView
        android:id="@+id/txteditadoConsumo"
        android:layout_width="90dp"
        android:layout_height="57dp"
        android:layout_marginLeft="310dp"
        android:layout_marginTop="635dp"
        android:layout_marginRight="10dp"
        android:background="@drawable/boarder"
        android:gravity="center" />



</RelativeLayout>